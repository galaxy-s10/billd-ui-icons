{
  "name": "billd-ui-icons",
  "version": "1.0.10",
  "description": "基于vue2.x构建的billd-ui组件库~",
  "homepage": "https://github.com/galaxy-s10/billd-ui-icons",
  "bugs": {
    "url": "https://github.com/galaxy-s10/billd-ui-icons/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/galaxy-s10/billd-ui-icons"
  },
  "license": "MIT",
  "author": "shuisheng <https://github.com/galaxy-s10>",
  "scripts": {
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0",
    "commit": "cz",
    "compile": "gulp -f ./build-tools/gulpfile.ts",
    "compile:dist": "gulp -f ./build-tools/gulpfile.ts --series icons-vue-icons dist",
    "compile:svg": "gulp -f ./build-tools/gulpfile.ts --series icons-svg-asn icons-svg-entry svg",
    "compile:vue": "gulp -f ./build-tools/gulpfile.ts --series icons-vue-icons vue",
    "dev": "webpack serve --config ./build-tools/webpack/webpack.common.js --env development",
    "dist": "webpack --config ./build-tools/webpack/webpack.common.js --env production",
    "lint": "eslint --config ./.eslintrc.js . --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:fix": "eslint --config ./.eslintrc.js --fix . --ext .js,.jsx,.ts,.tsx,.vue",
    "prepare": "husky install",
    "prettier": "prettier --write *",
    "release:local": "node ./scripts/release.js",
    "release:online": "node ./scripts/publish.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@huangshuisheng/icons-svg": "^1.0.4",
    "@huangshuisheng/icons-vue": "^1.0.9",
    "vue": "^2.6.14",
    "vue-fragment": "^1.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.14.7",
    "@rgrove/parse-xml": "^3.0.0",
    "@soda/friendly-errors-webpack-plugin": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.5",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "commitizen": "^4.2.4",
    "compression-webpack-plugin": "^8.0.1",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "eslint-webpack-plugin": "^3.0.1",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^2.0.9",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html-webpack-plugin": "^5.3.2",
    "html-webpack-tags-plugin": "^3.0.1",
    "husky": "^7.0.0",
    "inquirer": "^8",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "lodash": "^4.17.21",
    "mddir": "^1.1.1",
    "mini-css-extract-plugin": "^1.6.2",
    "node-emoji": "^1.10.0",
    "portfinder": "^1.0.28",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "semver": "^7.3.7",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.0.0",
    "svgo": "^2.3.1",
    "through2": "^4.0.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dashboard": "^3.3.5",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.0-3"
  }
}
